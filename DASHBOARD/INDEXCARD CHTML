<script type="text/javascript">

            $(document).ready(function () {
                $.ajax({
                    data: {},
                    url:'@Url.Action("indexcardA", "StudentDashboard")',
                    type: "GET",
                    dataType: "json",
                    success: function (data) {
                        var obj = JSON.stringify(data);
                        var result = JSON.parse(obj);
                        
                        var obj = data['indexcard'];
                        var barcode = data['barcode']

                        var image = data['image']
                        var signature = data['Signature']
                        
                        if (obj != null) {
                            //$('#tblDatatable').DataTable().destroy();
                            var print = "print";
                            var htm = '';
                            if (obj[0].intStatus == 0) {
                                $('#imgId').attr('src', barcode);
                                $('#imgId1').attr('src', barcode);
                                $('#imgOCPhoto').attr('src', image);
                                $('#imgCCPhoto').attr('src', image);
                                $('#imgOCSign').attr('src', signature);
                                $('#imgCCSign').attr('src', signature);
                                $('#lblOCRefNo').text(obj[0].vchApplicationNo);
                                $('#lblSECRefNo').text(obj[0].vchApplicationNo);
                                $('#lblOCCourse').text(obj[0].vchCourseName);
                                $('#lblSECourse').text(obj[0].vchCourseName);
                                $('#lblOCName').text(obj[0].vchApplicantName);
                                $('#lblSECName').text(obj[0].vchApplicantName);
                                $('#lblOCGender').text(obj[0].vchGender);
                                $('#lblSECGender').text(obj[0].vchGender);

                                var PFTDate = obj[0].fromDate + " from 7:00 AM to 5:30 PM   Group : " + obj[0].toDate;

                                // var PFTDate = " from 7:00 AM to 5:30 PM   Group : ";

                                $('#lblOCPFTDate').text(PFTDate);
                                $('#lblSECPFTDate').text(PFTDate);

                                $('#lblRefNo').text(obj[0].vchApplicationNo);
                                $('#lblRefNo1').text(obj[0].vchApplicationNo);
                                var course = "";
                                var gender = "";
                                var service = "";
                                var quota = "";
                                if (obj[0].vchCourseName == "B.P.Ed.")
                                    course = "B";
                                else if (obj[0].vchCourseName == "M.P.Ed.")
                                    course = "M";
                                else
                                    course = "";
                                if (obj[0].vchGender == "MALE")
                                    gender = "M";
                                else if (obj[0].vchGender == "FEMALE")
                                    gender = "W";
                                else
                                    gender = "";

                                if (obj[0].vchApplicantType == "FRESH")
                                    service = "FS";
                                else if (obj[0].vchApplicantType == "IN SERVICE")
                                    service = "INS";
                                else
                                    service = "";
                                if (obj[0].intReserveCategoryID == 1)
                                    quota = "PWD";
                                else
                                    quota = "";
                                var info = course + gender + service + quota;
                                $('#lblInfo').text(info);
                                $('#lblInfo1').text(info);
                                var lablea = "N.B:- Validation and Physical Fitness Test will be held on " + obj[0].fromDate + "(Group-" + obj[0].toDate + ")" +
                                    " from 7:00 AM to 5:30 PM at Kalinga Stadium Warm-up Practice Field (Running) with following documents in PHOTOCOPY & Original as well.";

                                var lableb = "N.B:- Validation and Physical Fitness Test will be held on " + obj[0].fromDate + "(Group-" + obj[0].toDate + ")" +
                                    " from 7:00 AM to 5:30 PM at Kalinga Stadium Warm-up Practice Field (Running) with following documents in PHOTOCOPY & Original as well.";
                                $('#lblNB1').text(lablea);
                                $('#lblNB2').text(lableb);
                            }

                            else if (obj[0].intStatus == 1) {
                                jAlert( "xyz",'<strong>You are not eligible to print Index Card !</strong>', function (r) {
                                    if (r) {

                                        window.location.href = 'StudentDashbaord';
                                        return true;
                                    }
                                    else {

                                        return false;
                                    }
                                });

                            }
                            else {
                                jAlert( "xyz",'<strong>You are not eligible to print Index Card !</strong>', function (r) {
                                    if (r) {

                                        window.location.href = 'StudentDashbaord';
                                        return true;
                                    }
                                    else {

                                        return false;
                                    }
                                });
                            }

                        }
                        else
                        {
                            if (result.state == "error") {

                                jAlert("xyz", result.message,  function (r) {
                                    if (r) {
                                        
                                        window.location.href = 'StudentDashbaord';
                                        return true;
                                    }
                                    else {
                                        
                                        return false;
                                    }
                                });
                                
                            }
                            else {
                                jAlert(result.message);
                            }
                        }
                       

                        
                        
                    },

                });

            });

</script>
